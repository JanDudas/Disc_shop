        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (newValue === '') {
        return;
    }
    var priceAllPieces = g_form.getIntValue('u_priceallpcs');
    g_form.clearOptions('u_priceallpcs');

    // var requestedCount = g_form.getIntValue('u_requestcount');
    var five = 5;
    var priceOfOneValue = g_form.getIntValue('u_price');
    var priceOfOnelabel = priceOfOneValue * five;
    var value = priceOfOnelabel * newValue;

    var allCount = g_form.getIntValue('u_count'); // 20 
    var restCount = allCount - newValue;

    if (restCount < 0) {
        g_form.addErrorMessage('JD restCount is zero or negative ' + restCount + ' Stop action.');
        return;
    }

    g_form.setValue('u_count', restCount);

    var lastValue = newValue;

    // g_form.addErrorMessage('JD priceOfOne je ' + priceOfOneValue);
    // g_form.addErrorMessage('JD newValue je ' + newValue);
    // g_form.addErrorMessage('JD label je ' + priceOfOnelabel);
    // g_form.addErrorMessage('JD hod je ' + hod);

    g_form.setValue('u_priceallpcs', value);


    if (isLoading && !g_form.isNewRecord()) {
        g_form.setValue('u_priceallpcs', priceAllPieces);
        g_form.setValue('u_count', allCount);
    }

    //Type appropriate comment here, and begin script below

}]]></script>
